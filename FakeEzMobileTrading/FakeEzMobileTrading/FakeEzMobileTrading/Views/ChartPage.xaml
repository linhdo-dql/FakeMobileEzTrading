<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FakeEzMobileTrading.Views.ChartPage"
             Title="Biểu đồ"
             xmlns:custom="clr-namespace:FakeEzMobileTrading.Views.CustomViews">
    <NavigationPage.TitleView>
        <StackLayout  Orientation="Horizontal" Padding="0,0,15,0">
            <Label Text="Biểu đồ" FontSize="20" TextColor="White" FontAttributes="Bold"
                   HorizontalOptions="StartAndExpand"/>
        </StackLayout>
    </NavigationPage.TitleView>
    <ContentPage.Content>
        <Grid>
            <custom:CustomWebView x:Name="webView" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
               
            </custom:CustomWebView>
        </Grid>
    </ContentPage.Content>
</ContentPage>